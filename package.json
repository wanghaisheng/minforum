    "seed": "node src/components/api/seed.js",
    "seed:only": "node src/components/api/seed-post.js",
    "seed:category": "node src/components/api/caegory.js",
    "build": "next build",
    "live": "yarn init-db && yarn build && NODE_ENV=production pm2 start serve.js --name ${PM2_APP_NAME:-'minforum'}",
